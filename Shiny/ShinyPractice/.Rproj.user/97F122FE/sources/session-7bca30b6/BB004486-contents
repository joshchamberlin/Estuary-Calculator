# This script is for practicing geocomputation. 
#Last edited: 2/5

library(tidyverse)
library(sf)
library(leaflet)


# Load Data -------------------------------
#Polygon of the Snohomish Delta
SnoDelta <- read_sf("data/SnoDelta")
st_crs(SnoDelta) #check CRS
SnoDelta <- st_transform(SnoDelta, crs = '+proj=longlat +datum=WGS84')

#Cedar Grove Mitigation project footprint
cedar_grove <- read_sf("data/CedarGroveMitigation")
st_crs(cedar_grove) #check CRS
cedar_grove <- st_transform(cedar_grove, crs = '+proj=longlat +datum=WGS84')

#Current Habitat Conditions
outlets <- read_sf("data/current_outlets_SNO")
outlets <- st_transform(outlets, crs = '+proj=longlat +datum=WGS84')
current_hab <- read_sf("data/current_hab_SNO")
current_hab <- st_transform(current_hab, crs = '+proj=longlat +datum=WGS84')

# Manipulate Data -------------------------
test <- st_intersection(x=cedar_grove, y=SnoDelta)
constituencies_wgs$intersects <- as.character(st_intersects(constituencies_wgs, polygon))

channels <- st_intersection(x = current_hab, y = cedar_grove)


